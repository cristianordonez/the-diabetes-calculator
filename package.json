{
   "name": "the-macro-trainer",
   "version": "1.2.0",
   "description": "Food Logging app to find foods with desired number of calories, carbs, protein and fat.",
   "main": "server.js",
   "scripts": {
      "lint": "eslint . --ext .ts,.tsx --fix",
      "jest": "jest",
      "cypress": "cypress open",
      "cypress-CLI": "npx cypress run --record --key 8fe96491-f3d2-4a36-93eb-6cd5688f93e0 ",
      "test": "concurrently -n 'jest,Cypress' -c 'bgGreen.bold,bgBlue.bold' \"npm:jest\" \"npm:cypress-CLI\" ",
      "server": "nodemon",
      "build": "webpack --config webpack.prod.js",
      "dev": "concurrently -n 'server, webpack' -c 'bgGreen.bold,bgBlue.bold' \"npm:server\" \"npx wait-on http://localhost:8080 && npm run webpack-dev\" ",
      "webpack-dev": "webpack serve --config webpack.dev.js",
      "build:server": "tsc",
      "start": "ts-node --transpile-only ./server/server.ts",
      "pm2": "sudo pm2 start npm --name 'themacrotrainer' -- start "
   },
